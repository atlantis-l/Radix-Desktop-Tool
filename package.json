{
  "name": "@atlantis-l/radix-desktop-tool",
  "version": "0.1.0",
  "description": "A desktop tool to interact with the radix network",
  "repository": {
    "type": "git",
    "url": "https://github.com/atlantis-l/Radix-Desktop-Tool"
  },
  "license": "MIT",
  "keywords": [
    "radix",
    "dlt",
    "crypto",
    "scrypto",
    "toolkit"
  ],
  "author": "atlantis-l",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "format": "prettier . --write",
    "build-mac": "prettier . --write && vue-tsc --noEmit && vite build && electron-builder --mac --x64",
    "build-win": "prettier . --write && vue-tsc --noEmit && vite build && electron-builder --win --x64",
    "test": "jest"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "engines": {
    "node": ">=20.8.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.6",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.8",
    "@vitejs/plugin-vue": "^4.4.0",
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-electron": "^0.14.1",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.3.7",
    "vue-tsc": "^1.8.20"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@atlantis-l/radix-tool": "^0.7.1",
    "ant-design-vue": "^4.0.6",
    "electron-context-menu": "^3.6.1",
    "file-saver": "^2.0.5",
    "pinia": "^2.1.7",
    "vue-router": "^4.2.5"
  }
}
